<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/main">

    <RelativeLayout
        android:id="@+id/login_page_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/content_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/page_login_page_1_ek1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View
                    android:id="@+id/_bg__login_page_1_ek2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/_bg__login_page_1_ek2_shape" />


                <Button
                    android:id="@+id/start_button"
                    android:layout_width="240dp"
                    android:layout_height="44dp"
                    android:text="Đăng ký"
                    android:textColor="@android:color/white"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:paddingLeft="32dp"
                    android:paddingRight="32dp"
                    android:paddingTop="16dp"
                    android:paddingBottom="16dp"
                    android:background="@color/xanh"
                    android:layout_below="@id/checkbox_nhothongtin"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="20dp" />


                <RelativeLayout
                    android:id="@+id/logo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="114dp"
                    android:layout_marginTop="77dp">

                </RelativeLayout>

                <TextView
                    android:id="@+id/dangnhap"
                    android:layout_width="120dp"
                    android:layout_height="38dp"
                    android:layout_gravity="center_horizontal|center"
                    android:gravity="center"
                    android:layout_marginLeft="125dp"
                    android:layout_marginTop="270dp"
                    android:fontFamily="Inter"
                    android:text="Đăng ký"
                    android:textColor="@color/_3_44_pm_color"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/viewtaikhoan"
                    android:layout_width="340dp"
                    android:layout_height="46dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="380dp"
                    android:background="@drawable/rectangle_38_shape" />

                <View
                    android:id="@+id/viewmatkhau"
                    android:layout_width="340dp"
                    android:layout_height="46dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="490dp"
                    android:background="@drawable/rectangle_39_shape" />

                <TextView
                    android:id="@+id/taikhoan"
                    android:layout_width="64dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center_horizontal|center"
                    android:gravity="center"
                    android:layout_marginLeft="13dp"
                    android:layout_marginTop="340dp"
                    android:fontFamily="Inter"
                    android:text="Nhập email"
                    android:textColor="@color/_3_44_pm_color"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/matkhau"
                    android:layout_width="102dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="450dp"
                    android:fontFamily="Inter"
                    android:text="Mật khẩu*"
                    android:textColor="@color/_3_44_pm_color"
                    android:textSize="16sp"
                    android:textStyle="bold" />



                <TextView
                    android:id="@+id/dacotaikhoan"
                    android:layout_width="173dp"
                    android:layout_height="23dp"
                    android:layout_gravity="center_horizontal|center"
                    android:gravity="center"
                    android:layout_marginLeft="115dp"
                    android:layout_marginTop="7dp"
                    android:fontFamily="Inter"
                    android:text="Đã có tài khoản? Đăng nhập"
                    android:textColor="@color/_3_44_pm_color"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:layout_below="@+id/start_button"/>

                <CheckBox
                    android:id="@+id/checkbox_nhothongtin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="35dp"
                    android:layout_marginTop="550dp"
                    android:fontFamily="Inter"
                    android:text="Nhớ thông tin"
                    android:textColor="@color/nh__th_ng_tin_color"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <RelativeLayout
                    android:id="@+id/logo_ek1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="80dp"
                    android:layout_marginTop="77dp">

                    <ImageView
                        android:id="@+id/logo_1_ek1"
                        android:layout_width="193dp"
                        android:layout_height="161dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="0dp"
                        android:contentDescription="logo_1_ek1"
                        android:src="@drawable/logo_1_ek1" />

                </RelativeLayout>


            </RelativeLayout>
        </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>
